<METADATAS>
    <METADATA SNAP_TYPE="host_storage" TARGET_TYPE="host" UI_IGNORE="Y" HISTORY_IGNORE="Y" COMPARE_IGNORE="Y">
        <METADATA_UI_NAME>Host Storage Layout</METADATA_UI_NAME>
        <TABLE NAME="MGMT_STORAGE_REPORT_DATA">
            <UI_NAME>Storage Data</UI_NAME>
            <COLUMN NAME="KEY_VALUE" TYPE="STRING" TYPE_FORMAT="256" IS_KEY="Y">Key Value</COLUMN>
            <COLUMN NAME="GLOAL_UNIQUE_ID" TYPE="STRING" TYPE_FORMAT="256" >Global Unique ID</COLUMN>
            <COLUMN NAME="NAME" TYPE="STRING" TYPE_FORMAT="64">Name</COLUMN>
            <COLUMN NAME="STORAGE_LAYER" TYPE="STRING" TYPE_FORMAT="32">Storage Layer</COLUMN>
            <COLUMN NAME="EM_QUERY_FLAG" TYPE="STRING" TYPE_FORMAT="64">EM Query Flag</COLUMN>
            <COLUMN NAME="ENTITY_TYPE" TYPE="STRING" TYPE_FORMAT="64">Entity Type</COLUMN>
            <COLUMN NAME="RAWSIZEB" TYPE="NUMBER">Raw Size</COLUMN>
            <COLUMN NAME="SIZEB" TYPE="NUMBER">Size</COLUMN>
            <COLUMN NAME="USEDB" TYPE="NUMBER">Used</COLUMN>
            <COLUMN NAME="FREEB" TYPE="NUMBER">Free</COLUMN>
            <COLUMN NAME="A1" TYPE="STRING" TYPE_FORMAT="256">A1</COLUMN>
            <COLUMN NAME="A2" TYPE="STRING" TYPE_FORMAT="256">A2</COLUMN>
            <COLUMN NAME="A3" TYPE="STRING" TYPE_FORMAT="256">A3</COLUMN>
            <COLUMN NAME="A4" TYPE="STRING" TYPE_FORMAT="256">A4</COLUMN>
            <COLUMN NAME="A5" TYPE="STRING" TYPE_FORMAT="256">A5</COLUMN>
            <COLUMN NAME="A6" TYPE="STRING" TYPE_FORMAT="256">A6</COLUMN>
            <COLUMN NAME="A7" TYPE="STRING" TYPE_FORMAT="256">A7</COLUMN>
            <COLUMN NAME="A8" TYPE="STRING" TYPE_FORMAT="256">A8</COLUMN>
            <COLUMN NAME="A9" TYPE="STRING" TYPE_FORMAT="256">A9</COLUMN>
            <COLUMN NAME="A10" TYPE="STRING" TYPE_FORMAT="256">A10</COLUMN>
            <COLUMN NAME="A11" TYPE="STRING" TYPE_FORMAT="256">A11</COLUMN>
            <COLUMN NAME="A12" TYPE="STRING" TYPE_FORMAT="256">A12</COLUMN>
            <COLUMN NAME="A13" TYPE="STRING" TYPE_FORMAT="256">A13</COLUMN>
            <COLUMN NAME="A14" TYPE="STRING" TYPE_FORMAT="256">A14</COLUMN>
            <COLUMN NAME="A15" TYPE="STRING" TYPE_FORMAT="256">A15</COLUMN>
            <COLUMN NAME="A16" TYPE="STRING" TYPE_FORMAT="256">A16</COLUMN>
            <COLUMN NAME="A17" TYPE="STRING" TYPE_FORMAT="256">A17</COLUMN>
            <COLUMN NAME="A18" TYPE="STRING" TYPE_FORMAT="256">A18</COLUMN>
            <COLUMN NAME="A19" TYPE="STRING" TYPE_FORMAT="256">A19</COLUMN>
            <COLUMN NAME="A20" TYPE="STRING" TYPE_FORMAT="256">A20</COLUMN>
        </TABLE>
         <TABLE NAME="MGMT_STORAGE_REPORT_KEYS">
                <UI_NAME>Storage Keys</UI_NAME>
                <COLUMN NAME="KEY_VALUE" TYPE="STRING" TYPE_FORMAT="256" IS_KEY="Y">Key Value</COLUMN>
                <COLUMN NAME="PARENT_KEY_VALUE" TYPE="STRING" TYPE_FORMAT="256">Parent Key Value</COLUMN>
         </TABLE>
         <TABLE NAME="MGMT_STORAGE_REPORT_ISSUES">
                <UI_NAME>Storage Issues</UI_NAME>
                <COLUMN NAME="TYPE" TYPE="STRING" TYPE_FORMAT="32" IS_KEY="Y">Type</COLUMN>
                <COLUMN NAME="MESSAGE" TYPE="STRING" TYPE_FORMAT="1000">Message</COLUMN>
          </TABLE>
    </METADATA>
</METADATAS>
